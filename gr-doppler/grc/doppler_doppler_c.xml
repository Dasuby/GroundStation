<?xml version="1.0"?>
<block>
  <name>Doppler Correction</name>
  <key>doppler_doppler_c</key>
  <category>Doppler</category>
  <import>import doppler</import>
  <make>doppler.doppler_c($*line1, $*line2, $lat, $lon, $alt, $year, $month, $day, $hour, $minute, $second, $microsecond)</make>

  <param>
    <name>TLE Line 1</name>
    <key>line1</key>
    <type>string</type>
  </param>
  <param>
    <name>TLE Line 2</name>
    <key>line2</key>
    <type>string</type>
  </param>
  <param>
    <name>Latitude</name>
    <key>lat</key>
    <type>float</type>
  </param>
  <param>
    <name>Longitude</name>
    <key>lon</key>
    <type>float</type>
  </param>
  <param>
    <name>Altitude</name>
    <key>alt</key>
    <type>float</type>
  </param>
  <param>
    <name>Year</name>
    <key>year</key>
    <type>int</type>
  </param>
  <param>
    <name>Month</name>
    <key>month</key>
    <type>int</type>
  </param>
  <param>
    <name>Day</name>
    <key>day</key>
    <type>int</type>
  </param>
  <param>
    <name>Hour</name>
    <key>hour</key>
    <type>int</type>
  </param>
  <param>
    <name>Minute</name>
    <key>minute</key>
    <type>int</type>
  </param>
  <param>
    <name>Second</name>
    <key>second</key>
    <type>int</type>
  </param>
  <param>
    <name>MicroSecond</name>
    <key>microsecond</key>
    <type>int</type>
  </param>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
